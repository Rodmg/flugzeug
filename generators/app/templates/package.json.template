{
  "name": "<%- name %>",
  "version": "1.0.0",
  "flugzeugVersion": "<%- flugzeugVersion %>",
  "description": "Flugzeug project",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "gulp",
    "watch": "gulp watch",
    "clean": "gulp clean",
    "seed": "gulp seed",
    "cleanSeed": "gulp cleanSeed",
    "migrate": "gulp migrate",
    "makeDocumentation":"gulp documentation",
    "makemigration": "gulp makemigration",
    "sql": "gulp sql",
    "start": "node --require source-map-support/register dist/main.js",
    "test": "gulp && mocha dist/test --recursive",
    "lint": "tsc --noEmit && eslint \"app/**/*.{js,ts}\" --fix",
    "format": "prettier --write \"app/**/*.{js,ts}\""
  },
  "author": "<%- author %>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/body-parser": "1.19.0",
    "@types/compression": "0.0.36",
    "@types/passport-microsoft": "0.0.0",
    "@types/ejs": "2.6.0",
    "@types/express": "4.17.8",
    "@types/helmet": "0.0.38",
    "@types/i18n": "0.8.7",
    "@types/ip": "0.0.31",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "7.2.8",
